      <%= yield %>
    </div><!--/span-->

    <div class="col-xs-6 col-sm-2 sidebar-offcanvas" id="sidebar" role="navigation">
      <div class="list-group">
        <%= link_to image_tag("logo/logo_200/tele.gif", alt: '"TELE"', title: "реле времени и контроля из Австрии", :class => "partners list-group-item"), :action => 'show', :controller => 'categories', :id => '142' %>
