            <li><%= link_to 'О компании', company_about_path %></li>
            <li><%= link_to 'Вакансии', company_vacancies_path %></li>
            <li><%= link_to 'Обмен ссылками', company_links_path %></li>
            <li><%= link_to 'Карта сайта', company_sitemap_path %></li>
            <li><%= link_to 'Контакты', company_contacts_path %></li>
          </ul>
        </li>
        <li><%= link_to '(812) 325-42-20', company_contacts_path, class: "phone" %></li>
      </ul>
    </div><!--/.nav-collapse -->
  </div>
</div>

