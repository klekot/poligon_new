<h1>Список видео</h1>

<table class="table table-striped">
  <tr>
    <th style="width: 80%;">Название</th>

    <th style="width: 20%;">Дата публикации</th>
    <th></th>
  </tr>

  <% @videos.each do |video| %>
      <tr>
        <td><%= link_to video.title, video %></td>

        <td><%= video.created_at.strftime("%d.%m.%Y") %></td>
      </tr>
  <% end %>
</table>
